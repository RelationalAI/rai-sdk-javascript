{
  "name": "@relationalai/rai-sdk-javascript",
  "description": "RelationalAI SDK for JavaScript",
  "version": "0.6.0",
  "author": {
    "name": "RelationalAI",
    "url": "https://relational.ai"
  },
  "homepage": "https://github.com/RelationalAI/rai-sdk-javascript#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RelationalAI/rai-sdk-javascript.git"
  },
  "license": "Apache-2.0",
  "keywords": [
    "api",
    "rel",
    "relationalai",
    "sdk"
  ],
  "browserslist": [
    "> 2%"
  ],
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/node/rai-sdk-javascript.cjs",
      "import": "./dist/node/rai-sdk-javascript.modern.js"
    },
    "./node": {
      "require": "./dist/node/rai-sdk-javascript.cjs",
      "import": "./dist/node/rai-sdk-javascript.modern.js"
    },
    "./web": {
      "require": "./dist/web/rai-sdk-javascript.cjs",
      "import": "./dist/web/rai-sdk-javascript.modern.js"
    }
  },
  "types": "./dist/typings/index.node.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/typings/index.node.d.ts"
      ],
      "node": [
        "./dist/typings/index.node.d.ts"
      ],
      "web": [
        "./dist/typings/index.web.d.ts"
      ]
    }
  },
  "main": "./dist/node/rai-sdk-javascript.cjs",
  "browser": "./dist/web/rai-sdk-javascript.cjs",
  "module": "./dist/node/rai-sdk-javascript.module.js",
  "unpkg": "./dist/web/rai-sdk-javascript.umd.js",
  "files": [
    "*"
  ],
  "scripts": {
    "prepack": "$npm_execpath run build",
    "build": "rm -rf ./dist && $npm_execpath run build:web && $npm_execpath run build:node && $npm_execpath run build:node:cjs",
    "build:web": "microbundle -i ./index.web.ts -o ./dist/web --target web --alias node-fetch=./fetch.ts --external none --define __RAI_SDK_VERSION__=$npm_package_version",
    "build:node": "microbundle -i ./index.node.ts -o ./dist/node --target node --define __RAI_SDK_VERSION__=$npm_package_version",
    "build:node:cjs": "$npm_execpath run build:node -- --format cjs --alias lodash-es=lodash,node-fetch=node-fetch-commonjs --generateTypes false",
    "proto-gen": "npx protoc --ts_out src/proto/generated/ --proto_path src/proto src/proto/*.proto",
    "dev:web": "$npm_execpath run build:web -- -w",
    "dev:node": "$npm_execpath run build:node -- -w",
    "format": "prettier --write ./src --loglevel warn",
    "tscheck": "tsc --noEmit --pretty true",
    "lint": "eslint --cache . --ext js --ext ts --ext tsx",
    "fix": "eslint --cache --fix . --ext js --ext ts --ext tsx",
    "test": "jest",
    "test:debug": "npx --node-arg=--inspect jest --runInBand --testTimeout=60000 --verbose --watch",
    "test:bundle": "node ./dist-test/esm-test.mjs && node ./dist-test/cjs-test.cjs",
    "example": "node --es-module-specifier-resolution=node --loader ts-node/esm"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "2.7.0",
    "@types/jest": "27.4.0",
    "@types/lodash-es": "4.17.4",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "commander": "8.3.0",
    "eslint": "7.28.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-license-header": "0.4.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "27.0.0",
    "jest": "27.4.7",
    "microbundle": "0.14.2",
    "nock": "13.2.2",
    "prettier": "2.2.1",
    "prettier-plugin-jsdoc": "0.3.8",
    "ts-jest": "27.1.2",
    "ts-node": "10.5.0",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "apache-arrow": "8.0.0",
    "config-ini-parser": "1.5.9",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21",
    "node-fetch": "3.1.1",
    "node-fetch-commonjs": "3.1.1",
    "query-string": "7.1.0"
  }
}
